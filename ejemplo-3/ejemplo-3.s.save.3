
.data

variable: .asciz "%d %d \r\n"

.text
.global _start
.global main
.extern printf     # Similar al include de C

main:
_start:

    
    subq $8, %rsp  # Alineamos los datos 
    
    mov $4, %rax
    mov $5, %rbx

    mov $mensaje, %rdi
    mov %rax, %rsi
    mov %rbx, %rdx

    # RAX se pone en 0 para indicar que necesitamos 0 registros de punto flotante
    # mov $0, %rax     # rax=0 Toca memoria
    xor %rax, %rax     # rax=0 No toca memoria

    call printf

    addq
